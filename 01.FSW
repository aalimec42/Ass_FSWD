...CSS
      *{
    box-sizing: border-box;
    margin: 0;
    padding: 0;
}

body{
      font-family:Arial, Helvetica, sans-serif;
      background-color: #f5f5f5;
      color: #333;
      line-height: 1.6;
      display:flex ;
      flex-direction: column;
      min-height: 100vh;
}

.header{
    background-color: #00ffff;
    color: #000000;
    padding: 10px;
    text-align: center;
    display: flex;
    align-items: center;
    flex-direction: column;
}

    .navbar{
        display: flex;
        justify-content:center ;
        flex-wrap: wrap;
}

.navbar a{
    text-decoration: none;
    padding: 7px;
    background-color: #1e9393;
    border-radius: 5px;
    margin: 5px;
    flex-grow: 1;
    text-align: center;
}

.navbar a:hover {
    background-color: white;
    color: #010101;
}

    .main-content{
        display: flex;
        flex-wrap:wrap ; 
        justify-content: center;
        align-items: stretch; 
        padding: 20px;
        flex-grow: 1;
}

.course{
    padding: 30px;
    background-color: bisque;
    border-radius: 10px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1); 
    margin: 10px;
    width: 300px;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.course img{
    width: 100%;
    height:auto;
    border-radius: 5px;
    margin-bottom: 10px;
}

.course h2{
    color:red;
    margin-bottom: 10px;
}

.footer{
    background-color: #00ffff;
    color: #000000;
    padding: 10px;
    text-align: center;
    display: flex;
    align-items: center;
    flex-direction: column;

}

.flex-container{
    display: flex;
    width: 100%;
    background-color:rebeccapurple ;
    border-radius: 10px;
    overflow: hidden;
}

.flex-item{
    padding: 30px;
    text-align: center;
}

.item1{
    flex-grow: 1;
    background-color: #4ca;
}
.item2{
    flex-grow: 2;
    background-color: blue;
}
    
.item3{
    flex-grow: 2;
    background-color: sienna;
}

@media (max-width:720px){
    .header h1{
        font-size:1.svh;
    }
    .navbar a{
        padding: 8px 15px;
        flex-grow: 1;
        margin: 3px; 
    }
   .course {
        width: 90%;
    }
